<?xml version="1.0" encoding="utf-8"?>
<manifest package="com.novoda.demo.edittextchips"
    xmlns:android="http://schemas.android.com/apk/res/android">

    <application
        android:icon="@drawable/icon"
        android:label="@string/app_name"
        android:theme="@style/Theme.AppCompat.Light">

        <activity
            android:name="com.novoda.demo.edittextchips.MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name="com.novoda.demo.edittextchips.EyeemActivity"
            android:label="@string/eyeem" />

        <activity
            android:name="com.novoda.demo.edittextchips.KlinkerActivity"
            android:label="@string/klinker" />

        <activity
            android:name="com.novoda.demo.edittextchips.DoodleActivity"
            android:label="@string/doodle" />

        <activity
            android:name="com.novoda.demo.edittextchips.PlumillionActivity"
            android:label="@string/plumillon" />

        <activity
            android:name="com.novoda.demo.edittextchips.SplitwiseActivity"
            android:label="@string/splitwise" />

    </application>
</manifest> 
